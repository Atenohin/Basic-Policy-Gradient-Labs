import gym
from gym import Wrapper
from gym import error, version
import os, json, logging, numpy as np, six

FILE_PREFIX = 'openaigym'
MANIFEST_PREFIX = FILE_PREFIX + '.manifest'

class DiscreteAction(Wrapper):
    def __init__(self, env):
        super(DiscreteAction, self).__init__(env)

    def _step(self, action):
        act = action[0]
        observation, reward, done, info = self.env.step(act)
 
    def _env_info(self):
        env_info = {
            'gym_version': version.VERSION,
        }
        if self.env.spec:
            env_info['env_id'] = self.env.spec.id
        return env_info
